{
  "python.defaultInterpreterPath": "${workspaceFolder}/backend/.venv/bin/python",
  "[python]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
  },
  "[javascript][typescript][javascriptreact][typescriptreact][json][jsonl][jsonc]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
  },
  "files.associations": {
    "*.css": "tailwindcss",
  },
}
